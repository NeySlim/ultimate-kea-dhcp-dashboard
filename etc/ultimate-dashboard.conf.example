# Ultimate Kea DHCP Dashboard Configuration

[DEFAULT]
# Dashboard server configuration
port = 8089
ssl_enabled = true
ssl_cert = /etc/ssl/certs/ssl-cert-snakeoil.pem
ssl_key = /etc/ssl/private/ssl-cert-snakeoil.key

# Kea DHCP configuration
# The dashboard automatically retrieves subnet and pool information from Kea via the control socket
kea_config = /etc/kea/kea-dhcp4.conf
kea_leases = /var/lib/kea/kea-leases4.csv

# Scanning configuration
scan_threads = 50
scan_timeout = 0.5

# SNMP configuration (optional)
snmp_community = public
snmp_enabled = false

# Language
language = fr
